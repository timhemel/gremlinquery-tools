#include gremtest.grm

test("astNodes_on_leaf", {
	identifiers =
	g.V().has('type','Function').has('code','test_astnodes')
	.functionToAST()
	.astNodes()
	.has('type','Identifier')
	.collect()
	subnodes = __.inject(identifiers[0])
		.astNodes()
		.collect()

	assertEquals(subnodes,[identifiers[0]])
})

run_tests()

